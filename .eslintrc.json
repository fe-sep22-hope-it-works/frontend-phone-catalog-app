{
  "extends": "@mate-academy/eslint-config-react-typescript",
  "rules": {
    "import/named": 2,
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "import/order": 2,
    "react/prop-types": "off",
    "import/no-cycle": [2, { "maxDepth": 2 }],
    "@typescript-eslint/no-explicit-any": ["off"]
  }
}
