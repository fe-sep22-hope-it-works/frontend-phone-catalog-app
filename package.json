{
  "name": "frontend-phone-catalog-app",
  "homepage": "https://fe-sep22-hope-it-works.github.io/frontend-phone-catalog-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "classnames": "^2.3.2",
    "gh-pages": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "react-scripts": "5.0.1",
    "sass": "^1.57.1",
    "swiper": "^8.4.5",
    "swiper": "^7.4.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build -u 'github-actions-bot <support+actions@github.com>'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-js": "eslint --ext .js,.jsx,.ts,.tsx ./src/ --ignore-path .eslintignore",
    "format": "prettier --write . --ignore-path .gitignore",
    "fix-style": "npm run format && npm run lint && npm run stylelint -- --fix",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint-fix": "stylelint \"**/*.scss\" --fix",
    "lint": "eslint . --ignore-path .eslintignore",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.tsx": "npm run lint",
    "*.ts": "npm run lint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react-typescript": "^1.0.11",
    "@mate-academy/students-ts-config": "*",
    "@mate-academy/stylelint-config": "0.0.9",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.1",
    "stylelint": "^13.5.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.17.2"
  }
}
